<!--Template of service board model defined in serices/metadata.json -->
<% _.each(services, function(service) { %>
<div class="api_item" id="service_board_<%= service.serviceName%>"
	title="<%= service.description%>" type="<%= service.serviceName%>">
	<img src="<%= service.icon%>" type="<%= service.serviceName%>"
		class="
		api_item_img " />
	<span class="api_item_text" type="<%= service.serviceName%>"><%= service.serviceName%></span>
</div>
<% }); %>